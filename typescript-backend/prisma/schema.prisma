datasource db {
  provider = "postgresql"
  url      = env("POSTGRES_DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id    Int     @id @default(autoincrement())
  firstName   String
  secondName  String
}

model Cafe {
  id          Int     @id @default(autoincrement())
  name        String
  rating      Float
}